      },
      {
        "id": 18108,
        "verse1": "بك استغثت أيا عوثي ومستندي",
        "verse2": "أنقذ عبيدك اعط العيش والرغدا"
      },
      {
        "id": 18109,
        "verse1": "ملك رقاب ذوي الأملاك أجمعهم",
        "verse2": "لخادم المصطفى وابطش بمن عندا"
      },
      {
        "id": 18110,
        "verse1": "وأيدن به الإسلام يعلو به الـ",
