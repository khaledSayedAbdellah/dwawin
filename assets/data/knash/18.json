      },
      {
        "id": 18134,
        "verse1": "وَتَغْمُرُنِي الْأَنْفَالُ مِنْ كُلِّ وِجْهَةٍ",
        "verse2": "فَتُثْمِرَ لِي الْعِرْفَانَ وَالشُّكْرَ وَالْهُدَى"
      },
      {
        "id": 18135,
        "verse1": "وَإنْ كَانَ يَرْقَى بِالْبَلَاءِ مُحَبَّبٌ",
        "verse2": "فَبِالشُّكْرِ أَرْقَى فَوْقَهُ شَاكِرًا يَدَا"
      },
      {
        "id": 18136,
        "verse1": "صَلَاةٌ وَتَسْلِيمٌ عَلَى خَيْرِ سَيِّدٍ",
        "verse2": "مَعَ الآلِ وَالْأَصْحَابِ كُلٌّ قَدِ اهْتَدَى"
      }
    ]
  },
  {
