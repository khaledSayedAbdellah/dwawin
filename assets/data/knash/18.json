      },
      {
        "id": 18165,
        "verse1": "لكي يصير ملجأ الأبرارِْ",
        "verse2": "وآية من واحد قهارُ"
      },
      {
        "id": 18166,
        "verse1": "ونقمة الكفار والفجار",
        "verse2": ""
      }
    ]
  },
  {
    "id": 1813,
    "diwanId": 18,
    "name": "استغاثة على حروف (وإذا مرضت فهو يشفين)",
    "rhyme": "ض",
    "content": [
      {
        "id": 18167,
        "verse1": "وَهْوَ الذِي يَشْفِي الْعُبَيْدَ إنْ مَرِضْ",
        "verse2": "ُغِيثُهُ لَوْ جَلَّ ذَلِكَ الْمَرَضْ"
      },
      {
        "id": 18168,
        "verse1": "إنِّي بِهِ إلَيْهِ فِي أَسْفَارِي ",
        "verse2": "كَمَا بِهِ إلَيْهِ فِي أَحْضَارِي"
      },
      {
        "id": 18169,
        "verse1": "ذَاكَ الْمُهَيْمِنُ الْمُغِيثُ الْكَافِي",
        "verse2": "وَهْوَ الْمُعَافِي وَالْقَوِيُّ الشَّافِي"
      },
      {
        "id": 18170,
        "verse1": "أَسْأَلُهُ شِفًا وَكُلَّ عَافِيَهُْ",
        "verse2": "مِنْ غَيْرِ شَرْطٍ وَبِحَقِّ الْوَاقِيَهْ"
      },
      {
        "id": 18171,
        "verse1": "مُنَّ عَلَيَّ يَا كَرِيمُ يَا عَلِي",
        "verse2": "وَيَا قَدِيرُ بِالشِّفَاءِ يَا وَلِيُ"
      },
      {
        "id": 18172,
        "verse1": "رَبَّ الْأَطِبَّاءِ وَرَبَّ الطِّبّ",
        "verse2": "تَشْفِي خَدِيمًا لِلْأَمِينِ الْحِبِّ"
      },
      {
        "id": 18173,
        "verse1": "ضُرٌّ خَفِيفٌ مَسَّنِي وَأَنْتَاّ",
        "verse2": "َرْحَمُ مَنْ يَرْحَمُ إذْ رَحِمْتَا"
      },
      {
        "id": 18174,
        "verse1": "تُزِيلُهُ بِسُرْعَةٍ فَإنِّي ",
        "verse2": "لَمْ آلَفِ الْبُؤْسَ كَفَيْتَ عَنِّيِّ"
      },
      {
        "id": 18175,
        "verse1": "فَضْلًا وَلُطْفًا أَذْهِبِ الْآلَامَاّ",
        "verse2": "وَأَذْهِبِ الْبَأْسَاءَ وَالْأَسْقَامَا"
      },
      {
        "id": 18176,
        "verse1": "هِيَ حَيَاتِي كُلُّهَا جَمَالُ ",
        "verse2": "وَعِنْدَ مَوْتِي تَمَّ لِي كَمَالُِّ"
      },
      {
        "id": 18177,
        "verse1": "وَلَيْسَ مِنْ خَيْرٍ عَمِلْتُ إنِّي",
        "verse2": "بَلْ فَضْلُ رَبِّيَ الْكَرِيمَ أَعْنِي"
      },
      {
        "id": 18178,
        "verse1": "يَشْفِي إلَهِي مَرَضِي وَسَقَمِي",
        "verse2": "وَعِلَّتِي وَيُذْهِبَنَّ غَمِّيِّ"
      },
      {
        "id": 18179,
        "verse1": "شِفًا عَمِيمًا لَا يُبَقِّي أَلَمَا",
        "verse2": "أَوْ عِلَّةً يُبْقِي الْعُبَيْدَ سَلَمَا"
      },
      {
        "id": 18180,
        "verse1": "فَفِيهِ أَمَلِي أَجَلْ وَثِقَتِيّ",
        "verse2": "بِأَنَّهُ الشَّافِي لِكُلِّ عِلَّةِِّ"
      },
      {
        "id": 18181,
        "verse1": "يَا رَبِّ بِالْهَادِي وَبِالْأَمِينِّ",
        "verse2": "وَبِالْمُزَمِّلِ وَبِالْمَامُونِّ"
      },
      {
        "id": 18182,
        "verse1": "نَرُومُ رَقْعَ خَالِقِي لِي خَرْقَاّ",
        "verse2": "فَلَا أُعَانِي أَلَمًا وَحَرْقَا"
      },
      {
        "id": 18183,
        "verse1": "يَا رَبِّ صَلِّيَنْ عَلَى الْمُدَّثِّرِ",
        "verse2": "وَصَحْبِهِ وَالآلِ أَهْلِ الْكَوْثَرِ"
      }
    ]
  },
  {
    "id": 1814,
    "diwanId": 18,
    "name": "توسل على حروف ( رب إني مغلوب فانتصر)",
    "rhyme": "ض",
    "content": [
      {
        "id": 18184,
        "verse1": "رب العباد قاهر الجميع",
        "verse2": "بك وبنبيك الشفيعْ"
      },
      {
        "id": 18185,
        "verse1": "بالذات والصفات والأسماء",
        "verse2": "وبالذي في الأرض والسماء "
      },
      {
        "id": 18186,
        "verse1": "بكوني المقصر المقرا",
        "verse2": "وكوني الجاني ولا مفرا"
      },
      {
        "id": 18187,
        "verse1": "أهلك إلهي من يكيد دينكاُْ",
        "verse2": "وشتتن شملا له بحولكاْ"
      },
      {
        "id": 18188,
        "verse1": "نرى هلاكهم وذلهم أجل",
        "verse2": "وعزهم طرا تلاشى واضمحلُ"
      },
      {
        "id": 18189,
        "verse1": "نرث أرضهم وأمرهم ولم ",
        "verse2": "ينل جميعهم سوى عار وذم"
      },
      {
        "id": 18190,
        "verse1": "يجعل بأسهم وشرهم بهمّ",
        "verse2": "يخيب الرجا لكل حزبهم"
      },
      {
        "id": 18191,
        "verse1": "متين شدد وطأة على الغوي",
        "verse2": "لؤو وسيس والذي لم يرعو"
      },
      {
        "id": 18192,
        "verse1": "غير وزلزل شتتن ودمرّ",
        "verse2": "وخالفن بين قلوب الزمر"
      },
      {
        "id": 18193,
        "verse1": "لا تبق منهم لا تذر عريبا",
        "verse2": "أرسل عليهم صرصرا غريباُِّ"
      },
      {
        "id": 18194,
        "verse1": "وسلطن عليهم عدوا",
        "verse2": "لا يرحم الذي عتا عتوا"
      },
      {
        "id": 18195,
        "verse1": "بجمعهم وفردهم فأنزل",
        "verse2": "صاعقة واعرهم في المنزل "
      },
      {
        "id": 18196,
        "verse1": "فقد كفيت أحمد الأحزابا",
        "verse2": "فأنزل العقاب والعذابا"
      },
      {
        "id": 18197,
        "verse1": "إلى جموع حاربوا الإسلاما",
        "verse2": "وسلط الأوجاع والآلاما"
      },
      {
        "id": 18198,
        "verse1": "نراهم في سخطة وغضب",
        "verse2": "وكمد وأسف نصب"
      },
      {
        "id": 18199,
        "verse1": "تغري عليهم الذي لا يرحم ",
        "verse2": "بؤسهم وكلهم يغرم"
      },
      {
        "id": 18200,
        "verse1": "صب عليهم نكدا ولتهتكِ",
        "verse2": "أستارهم وبددن ولتهلك"
      },
      {
        "id": 18201,
        "verse1": "رأسهم في أمد قريب ",
        "verse2": "منك بسهم نحوه ومصيب"
      },
      {
        "id": 18202,
        "verse1": "ثم صلاة الله والسلام ",
        "verse2": "على الذي انمحى به الظلام"
      }
    ]
  },
  {
    "id": 1815,
    "diwanId": 18,
    "name": "استغاثة على حروف ( ويشف صدور قوم مؤمنين)",
    "rhyme": "ض",
    "content": [
      {
        "id": 18203,
        "verse1": "وَإنَّمَا أَشْكُو لِرَبِّي الْعَالِمِ ",
        "verse2": "بَثِّي وَحُزْنِي لَا إلَى الْعَوَالِمِْ"
      },
      {
        "id": 18204,
        "verse1": "يُمَتِّعُ الْعَبْدَ بِقُوَّةٍ وَفَضْلْ",
        "verse2": "فَلَمْ أَجِدْ لَا قُوَّةً وَلَا حِوَلْ "
      },
      {
        "id": 18205,
        "verse1": "شِفًا قَرِيبًا وَامْتِدَادَ الْعَافِيَهْ",
        "verse2": "وَالْعَيْشَ فِي لُطْفٍ لِيَوْمِ الْقَاضِيَهْ"
      },
      {
        "id": 18206,
        "verse1": "فَكُلُّ هَوْلٍ يَهْزِمُ اصْطِبَارِي ",
        "verse2": "لَمْ أَتَعَوَّدْ صَدْمَةَ اخْتِبَارِ"
      },
      {
        "id": 18207,
        "verse1": "صَدْرِي يَضِيقُ عِنْدَ أَدْنَى كَدَر",
        "verse2": "يَا رَبِّ لُطْفًا عِنْدَ كُلِّ قَدَرُِ"
      },
      {
        "id": 18208,
        "verse1": "دَرَيْتَنِي يَرُوقُ لِي الْجَمَالُ ",
        "verse2": "فِي كُلِّ مَظْهَرٍ وَلَا أَزَالُ"
      },
      {
        "id": 18209,
        "verse1": "وَقَوِّنِي بِالْمُصْطَفَى الْمُخْتَارِّ",
        "verse2": "رَبَّ الْعِبَادِ وَاهِبِي مُخْتَارِي"
      },
      {
        "id": 18210,
        "verse1": "رَقِّ عُبَيْدَكَ بِشُكْرِكَ الْحَسَنْ",
        "verse2": "َكُلُّ مَا تَصْنَعُهُ فَهْوَ حَسَنْ"
      },
      {
        "id": 18211,
        "verse1": "قَوِّمْ لِيَ الْأَوْضَاعَ وَالْأَحْوَالَا",
        "verse2": "َواجْعَلْ حَيَاتِي كُلَّهَا امْتِثَالَا"
      },
      {
        "id": 18212,
        "verse1": "وَارْفَعْ بِيَ الْإسْلَامَ وَانْفَعِ الْمَلَا",
        "verse2": "بِعَبْدِكَ الْحَقِيرِ وَادْفَعِ الْبَلَا"
      },
      {
        "id": 18213,
        "verse1": "مَتِينُ يَا مُعِينُ يَا مُهَيْمِنُ",
        "verse2": "قَادِرُ يَا عَزِيزُ يَا ذَا الْمُومِنُ"
      },
      {
        "id": 18214,
        "verse1": "مِنْ سَيِّءِ الْأَسْقَامِ وَالْأَمْرَاضِ",
        "verse2": "قِنِي حَيَاتِي وَمِنَ الْأَغْرَاضِ"
      },
      {
        "id": 18215,
        "verse1": "مَنَّانُ يَا حَنَّانُ يَا مُدَبِّرُ",
        "verse2": "فَلْتَكْفِ ذَا الْحَقِيرَ مَنْ يُدَبِّرُ"
      },
      {
        "id": 18216,
        "verse1": "وَلْتَكْفِهِ وَلْتَكْفِهِ يَا كَافِي",
        "verse2": "وَعَافِهِ فَإنَّكَ الْمُعَافِي"
      },
      {
        "id": 18217,
        "verse1": "مَدَدْتُ كَفِّي نَحْوَ رَبِّ الْمُصْطَفَى ",
        "verse2": "صَلَّى عَلَيْهِ رَبُّنَا وَشَرَّفَا"
      },
      {
        "id": 18218,
        "verse1": "نَافِي الضَّلَالِ مُرْشِدُ الْعِبَادِ",
        "verse2": "خَيْرِ الْوَرَى وَمَطْلَبِي مُرَادِي"
      },
      {
        "id": 18219,
        "verse1": "يَقُومُ لِي عِنْدَ الْإلَهِ الْخَالِقِ ِ",
        "verse2": "عَلَيْهِ صَلَّى عَدَدَ الْخَلَائِقِ"
      },
      {
        "id": 18220,
        "verse1": "نَعَمْ وَيَشْفِي سَقَمِي وَكَرْبِي",
        "verse2": "وَيَجْمَعَنْ  لِي طَلَبِي وَأَرَبِي"
      }
    ]
  },
  {
    "id": 1816,
    "diwanId": 18,
    "name": " استغاثة",
    "rhyme": "ن",
    "content": [
      {
        "id": 18221,
        "verse1": "أسأل اللهم يا رحمن ",
        "verse2": "ويا مهيمن ويا منانِْ"
      },
      {
        "id": 18222,
        "verse1": "هب لي في الظهر مناء قلبيْ",
        "verse2": "ولتغفرن يا رب كل ذنبي "
      },
      {
        "id": 18223,
        "verse1": "ملكا كبيرا بغتة وفيضاْ",
        "verse2": "ولترفعن بالبسط هذا الفيضا"
      },
      {
        "id": 18224,
        "verse1": "سلم إلهي ديننا وسددا",
        "verse2": "أقوالنا وفعلنا وأيدا"
      },
      {
        "id": 18225,
        "verse1": "قهرا وصولة على الأعداء ",
        "verse2": "وشددن عليهم بأسائيُِ"
      },
      {
        "id": 18226,
        "verse1": "كف العدى وزلزلن أركانهم",
        "verse2": "ولتولنا من بعدهم مكانهمُ"
      },
      {
        "id": 18227,
        "verse1": "حصنا منيعا منك يا حفيظ",
        "verse2": "وافعل بنا ما للعدى يغيظ"
      },
      {
        "id": 18228,
        "verse1": "لطفا وعونا منك يا  لطيف",
        "verse2": "ولتذهب الهم الذي يطيف"
      },
      {
        "id": 18229,
        "verse1": "عالم حالي وخفي سري",
        "verse2": "َفبدلن باليسر كل عسر"
      },
      {
        "id": 18230,
        "verse1": "يامن أمرت بالدعا فأجب",
        "verse2": "وعجل الفرج قبل التعب"
      },
      {
        "id": 18231,
        "verse1": "صل أيا رب وسلم أبدا",
        "verse2": "على النبي الهاشمي أحمداُ"
      }
    ]
  },
  {
    "id": 1817,
    "diwanId": 18,
    "name": "نظم نيل المراد في التوسل بسور القرآن الكريم",
    "rhyme": "ن",
    "content": [
      {
        "id": 18232,
        "verse1": "الحمد لله على القرآن",
        "verse2": "وجعله الشفاء للأذهانِْ"
      },
      {
        "id": 18233,
        "verse1": "وللصدور والجوارح وماْ",
        "verse2": "ينزل من داء يرى أو علما "
      },
      {
        "id": 18234,
        "verse1": "ثم على الآتي به عليه ما",
        "verse2": "من الصلاة عد قول العلما"
      },
      {
        "id": 18235,
        "verse1": "ثم على قول ادعوني أستجب",
        "verse2": "ثم ابتغوا وسيلة لما طلب"
      },
      {
        "id": 18236,
        "verse1": "وأفضل الوسائل القرآن",
        "verse2": "لأنه الشفاء والبرهان"
      },
      {
        "id": 18237,
        "verse1": "هذا وإني قد أردت نظما",
        "verse2": "بجمع كل سورة منه اسماُ"
      },
      {
        "id": 18238,
        "verse1": "توسلا بجاهه وجاه ما",
        "verse2": "فيه من الأسماء من رب السما"
      },
      {
        "id": 18239,
        "verse1": "والله أرجو أن يكون نافعا",
        "verse2": "وخالصا له وضرا دافعا"
      },
      {
        "id": 18240,
        "verse1": "رب به ففرجن كربنا",
        "verse2": "ورد كيد من يروم حربنا"
      },
      {
        "id": 18241,
        "verse1": "ووالنا بالنصر والآلاء",
        "verse2": "والعز والحفظ من البلاء"
      },
      {
        "id": 18242,
        "verse1": "وعافنا واقض لنا الحوائجا",
        "verse2": "واجعل لنا من كل ضيق مخرجا"
      },
      {
        "id": 18243,
        "verse1": "واجعل لنا في قلب كل من يرى",
        "verse2": "ومن بنا يسمع ودا غمرا"
      },
      {
        "id": 18244,
        "verse1": "واهد بنا واهد إلينا البشرا",
        "verse2": "بالخير واجعل صيتنا منتشرا"
      },
      {
        "id": 18245,
        "verse1": "ولتقض همي ظاهرا وسترا",
        "verse2": "بسر الفاتحة ثم البقرا"
      },
      {
        "id": 18246,
        "verse1": "وآل عمران وبالنساء",
        "verse2": "مائدة فوال بالعطاء"
      },
      {
        "id": 18247,
        "verse1": "كذاك بالأنعام والأعراف",
        "verse2": "الانفال جد لنا بخير واف"
      },
      {
        "id": 18248,
        "verse1": "وتوبة ويونس وهود",
        "verse2": "يوسف والرعد لنا فجودي"
      },
      {
        "id": 18249,
        "verse1": "وبالخليل وبحجر النحل",
        "verse2": "الاسراء والكهف فزد بالفضل"
      },
      {
        "id": 18250,
        "verse1": "ومريم وطه الأنبياء",
        "verse2": "والحج فانصرنا على الأعداء"
      },
      {
        "id": 18251,
        "verse1": "بالمؤمنين النور سورة الفرقان",
        "verse2": "والشعراء النمل جد بالإحسان"
      },
      {
        "id": 18252,
        "verse1": "وقصص والعنكبوت الروم",
        "verse2": "لقمان سجدة فاحفظ لي قومي"
      },
      {
        "id": 18253,
        "verse1": "الأحزاب ثم سبإ ملائكه",
        "verse2": "يس من يعادينا فأهلكه"
      },
      {
        "id": 18254,
        "verse1": "ثم بالصافات وص والزمر",
        "verse2": "وغافر وفصلت والنور"
      },
      {
        "id": 18255,
        "verse1": "وبالشورى والزخرف الدخان",
        "verse2": "جاثية الأحقاف خذ بياني"
      },
      {
        "id": 18256,
        "verse1": "وبمحمد وفتح حجرات",
        "verse2": "اقض حوائجي بق ذاريات"
      },
      {
        "id": 18257,
        "verse1": "بالطور والنجم كذا والقمر",
        "verse2": "بسورة الرحمن زك عمري"
      },
      {
        "id": 18258,
        "verse1": "أدم علي نعما بالواقعه",
        "verse2": "وسورة الحديد والمجادله"
      },
      {
        "id": 18259,
        "verse1": "والحشر ممتحنة والصف قف",
        "verse2": "بجمعة منافقين عنا كف"
      },
      {
        "id": 18260,
        "verse1": "تغابن وبالطلاق ربنا",
        "verse2": "وسورة التحريم والملك اهدنا"
      },
      {
        "id": 18261,
        "verse1": "نون وحاقة معارج ونوح",
        "verse2": "جن فسخر لي كل ما يلوح"
      },
      {
        "id": 18262,
        "verse1": "وبمزمل مدثر فلا",
        "verse2": "يقابلنا بالقيامة بلا"
      },
      {
        "id": 18263,
        "verse1": "وعافنا بالدهر ثم المرسلات",
        "verse2": "وسورة النبإ ثم النازعات"
      },
      {
        "id": 18264,
        "verse1": "عبس والتكوير الانفطار",
        "verse2": "كذاك بالتطفيف زد أنواري"
      },
      {
        "id": 18265,
        "verse1": "بالانشقاق والبروج طارق",
        "verse2": "الأعلى وغاشية فجر وافق"
      },
      {
        "id": 18266,
        "verse1": "ببلد شمس وليل والضحى",
        "verse2": "والانشراح لي والتين أصلحا"
      },
      {
        "id": 18267,
        "verse1": "بعلق والقدر ثم البينه",
        "verse2": "وزلزلت أمري عليه بينه"
      },
      {
        "id": 18268,
        "verse1": "بالعاديات ربنا والقارعه",
        "verse2": "تكاثر والعصر بشرى رائعه"
      },
      {
        "id": 18269,
        "verse1": "همزة فيل قريش ربي",
        "verse2": "كذاك بالماعون اكشف كربي"
      },
      {
        "id": 18270,
        "verse1": "بكوثر والكافرون النصر",
        "verse2": "تبت والإخلاص فأظهر نصري"
      },
      {
        "id": 18271,
        "verse1": "بسورة الفلق ثم الناس",
        "verse2": "فلتكف شر غافل وناس"
      },
      {
        "id": 18272,
        "verse1": "وبالغنى فوافنا بالعافيه",
        "verse2": "في الدين والدنيا وفي ذي الآتيه"
      },
      {
        "id": 18273,
        "verse1": "ولتسقنا من كأس ختم الأوليا",
        "verse2": "ووالنا بالحب منك واليا"
      },
      {
        "id": 18274,
        "verse1": "واسق المواليد بنور الحب",
        "verse2": "واغفر لنا ولهم كل ذنب"
      },
      {
        "id": 18275,
        "verse1": "واحفظ إلهي ظاعنا وقاطنا",
        "verse2": "منا ولا تخيبن ظننا"
      },
      {
        "id": 18276,
        "verse1": "بالخير والضر فنح عنا",
        "verse2": "وأهله فنح حيث كنا"
      },
      {
        "id": 18277,
        "verse1": "وعلمنا من لدنك علما",
        "verse2": "ينفع من للمسلمين ينمى"
      },
      {
        "id": 18278,
        "verse1": "وبلدا نحن به فعافه",
        "verse2": "وارزقه غيثا بالهنا ووافه"
      },
      {
        "id": 18279,
        "verse1": "وكل من يسكنه فزنه",
        "verse2": "والقحط والعدو فاصرف عنه"
      },
      {
        "id": 18280,
        "verse1": "واشف من استشفى بنا وارزق له",
        "verse2": "مراده واحفظه واحفظ ماله"
      },
      {
        "id": 18281,
        "verse1": "أغث بنا من استغاث أبدا",
        "verse2": "وارزقه ما يحبه وأرشدا"
      },
      {
        "id": 18282,
        "verse1": "وسخر النساء والرجالا",
        "verse2": "أصلح لنا الأحوال والأموالا"
      },
      {
        "id": 18283,
        "verse1": "واردد أزمة الشوارد إلى",
        "verse2": "قبضة أيدينا إلهي مسجلا"
      },
      {
        "id": 18284,
        "verse1": "واجعل جميع الخلق منا راجيا",
        "verse2": "خيرا وخيرك لنا فواليا"
      },
      {
        "id": 18285,
        "verse1": "واجعلهم إلهي منا حذرين",
        "verse2": "ولنك فيك راغبين راهبين"
      },
      {
        "id": 18286,
        "verse1": "وفضلك ابسطه على الجماعه",
        "verse2": "واجعل لها رب التقى بضاعه"
      },
      {
        "id": 18287,
        "verse1": "ولم شملها ووفقنها",
        "verse2": "وبالغنى والنصر أيدنها"
      },
      {
        "id": 18288,
        "verse1": "يا رب يا رب وكمل قصدنا",
        "verse2": "وقصد من أحبنا يا ربنا"
      },
      {
        "id": 18289,
        "verse1": "ومن لنظمنا قرا وسمعا",
        "verse2": "ومن يؤمن ومن به دعا"
      },
      {
        "id": 18290,
        "verse1": "وارزق له جميع ما أرادا",
        "verse2": "به وواليا به ازديادا"
      },
      {
        "id": 18291,
        "verse1": "واختم بحسنى ربنا لنا ولا",
        "verse2": "تشمت بنا عدونا يا ذا العلا"
      },
      {
        "id": 18292,
        "verse1": "والمؤمنين كلهم بكل ما",
        "verse2": "به دعوت ربنا فعمما"
      },
      {
        "id": 18293,
        "verse1": "سميته نيل المراد للذي",
        "verse2": "دعا به لكل أمر يحتذي"
      },
      {
        "id": 18294,
        "verse1": "وصل ما قيلت صلاة أبدا",
        "verse2": "على النبي الهاشمي سرمدا"
      },
      {
        "id": 18295,
        "verse1": "عليه مني الصلاة والسلام",
        "verse2": "ما دامت الدنيا وجنة السلام"
      }
    ]
  },
  {
    "id": 1818,
    "diwanId": 18,
    "name": " إلَى اللهِ أَشْكُو مَا أُلَاقِي مِنَ الشَّكْوَى",
    "rhyme": "و",
    "content": [
      {
        "id": 18296,
        "verse1": "إلَى اللهِ أَشْكُو مَا أُلَاقِي مِنَ الشَّكْوَى",
        "verse2": "فَلَا وَزَرٌ أَرْجُو سِوَاهُ وَلَا مَأْوَى"
      },
      {
        "id": 18297,
        "verse1": "هُمُومِي إلَى مَوْلَايَ أَرْجُو انْفِرَاجَهَا",
        "verse2": "فَفَقْرِي إلَى مَوْلَايَ فِي الْجَهْرِ وَالنَّجْوَى"
      },
      {
        "id": 18298,
        "verse1": "مَقَالِي وَحَالِي يَجْزِيَانِ لِفَاقَتِي",
        "verse2": "وَيَقْوَى رَجَائِي حِيَن عَنْ غَيْرِهِ يُطْوَى"
      },
      {
        "id": 18299,
        "verse1": "سَأَلْتُكَ يَا كَافِي الْعَبِيدِ بِأَسْرِهَا",
        "verse2": "ِأَنْ تُحْسِنَ الْعُقْبَى وَأَنْ تَرْفَعَ الْبَلْوَى"
      },
      {
        "id": 18300,
        "verse1": "قَدِيرُ قَدِيرُ يَا إلَهِي وَإنَّنِي ",
        "verse2": "فَقِيرٌ ضَعِيفٌ أَشْكِ شَكْوَى أَخِي شَكْوَى"
      },
      {
        "id": 18301,
        "verse1": "كَفَانِي كَفَانِي أَنْ مَدَدْتُ يُدَيَّتِي ",
        "verse2": "إلَى حَضْرَةِ الْعَلْيَا وَأَنْحُو لَهَا نَحْوَاُ"
      },
      {
        "id": 18302,
        "verse1": "عَلَى الْمُصْطَفَى الْمَنْصُورِ أَزْكَى صَلَاتِهِ",
        "verse2": "مَعَ الْآلِ وَالْأَصْحَابِ مَا أَكْثَرَ الْجَدْوَى"
      }
    ]
  }
]