[
  {
    "id": 1801,
    "diwanId": 18,
    "name": "توسل على حروف ( والله يعصمك من الناس)",
    "rhyme": "ب",
    "content": [
      {
        "id": 18001,
        "verse1": "والله يعصم عبدا بان معتربا",
        "verse2": "لم ياو إلا إليه الدهر مكتئبا"
      },
      {
        "id": 18002,
        "verse1": "أنت الغياث وأنت الحي منك بقا",
        "verse2": "ي أو أنال مرادي ملجأ الغربا"
      },
      {
        "id": 18003,
        "verse1": "لا تتركني في مرمى العدا غرضاٍ",
        "verse2": "وأنت يا رب تقضي للورى أربا"
      },
      {
        "id": 18004,
        "verse1": "لا تمتحني بما يعيا الفؤاد به",
        "verse2": "ولا تقدر علينا ما الإله أبى "
      },
      {
        "id": 18005,
        "verse1": "هب لي بقاء به قد سر وامقنا",
        "verse2": "وسيء باغضنا يا من ينيل حبا"
      },
      {
        "id": 18006,
        "verse1": "يا من تجير فاقهر كل طاغيه",
        "verse2": "واحفظ عبدا ضعيفا واكشف الكربا"
      },
      {
        "id": 18007,
        "verse1": "عصمتم كل ذي يتم وذي وجل",
        "verse2": "من الخلائق أو من كان مغتربا"
      },
      {
        "id": 18008,
        "verse1": "صبرت أفعال أقوام ذوي ضغن",
        "verse2": "إذ أنت ملتجإي مهما الجواد كبا"
      },
      {
        "id": 18009,
        "verse1": "ما لي كفيل ولا ملجا ولا وزر",
        "verse2": "ولا حميم سوى الرحمن ما عزبا"
      },
      {
        "id": 18010,
        "verse1": "كفى به من معين استعين به",
        "verse2": "وهو اللطيف الخبير ما سواه هبا"
      },
      {
        "id": 18011,
        "verse1": "من كنت عونا له لا ضير يقصده",
        "verse2": "ولو رماه جميع الخلق لا كذبا"
      },
      {
        "id": 18012,
        "verse1": "أكون مستترا في ظل جنبكم",
        "verse2": "أما الخلائق فاضرب بيننا حجبا"
      },
      {
        "id": 18013,
        "verse1": "أراهم عدما محضا وشرهم",
        "verse2": "أمنت منه فربي يقبل الطلبا"
      },
      {
        "id": 18014,
        "verse1": "لما تفاخر بالأموال كل فتى",
        "verse2": "جعلت فخري افتقاري للذي كتبا"
      },
      {
        "id": 18015,
        "verse1": "ناجيت ربي في أمري ويعرفه",
        "verse2": "ويعرف الهم ربي الخارق الحجبا"
      },
      {
        "id": 18016,
        "verse1": "ناديت ربي كئيبا طالبا أربا",
        "verse2": "وهو الجدير بأن يرجى وأن يهبا"
      },
      {
        "id": 18017,
        "verse1": "أعط العبيد مراما سرعة وكفا",
        "verse2": "ني علمكم أربي عن ذكري الأربا"
      },
      {
        "id": 18018,
        "verse1": "سألت ربي صلاة ثم تسلمة",
        "verse2": "على النبي ثم آل ثم من صحبا"
      }
    ]
  },
  {
    "id": 1802,
    "diwanId": 18,
    "name": "توسل على حروف ( فسيكفيكهم الله وهو السميع العليم)",
    "rhyme": "ب",
    "content": [
      {
        "id": 18019,
        "verse1": "فزعت للكافي المهيمن الرقيب",
        "verse2": "فكف أيدي البغى رب لي قريب"
      },
      {
        "id": 18020,
        "verse1": "سيرت آمالي إلى الكريم",
        "verse2": "فانزاح كربي بالولي العظيم"
      },
      {
        "id": 18021,
        "verse1": "يممته بالذل والفقر  المقيم",
        "verse2": "وهو الجليل القادر الغني الرحيم"
      },
      {
        "id": 18022,
        "verse1": "كفى ويكفي أنه الكافي الكبير",
        "verse2": "عبدا ذليلا عاصيا وهو فقير"
      },
      {
        "id": 18023,
        "verse1": "فرق جيوش الهم والنكير",
        "verse2": "ورد كيد حاسد، منيري"
      },
      {
        "id": 18024,
        "verse1": "يرزقني سعادة الدارين ",
        "verse2": "ومن قفا نهجي من الحزبين"
      },
      {
        "id": 18025,
        "verse1": "كن يا إلهي وزري وكن لي ",
        "verse2": "وافتح علي رب كل قفل"
      },
      {
        "id": 18026,
        "verse1": "هذا الذي أنعمت زد وكمل",
        "verse2": "أدم عطا عودتني وأجزل"
      },
      {
        "id": 18027,
        "verse1": "متين يا جواد يا كريم ",
        "verse2": "مجيب يا رؤوف يا رحيم"
      },
      {
        "id": 18028,
        "verse1": "أنت الذي أسديت كل خير ",
        "verse2": "أنت الذي يسرت كل مير"
      },
      {
        "id": 18029,
        "verse1": "لا تقطع الإحسان عمن قد أسا",
        "verse2": "وادفع عن العبد المخوف والأسى"
      },
      {
        "id": 18030,
        "verse1": "اعط عطاء غير منقوص وزد",
        "verse2": "أورثني الأرض وأولني الرشد"
      },
      {
        "id": 18031,
        "verse1": "هو السميع وهو العليم",
        "verse2": "وهو اللطيف القادر الحكيم"
      },
      {
        "id": 18032,
        "verse1": "وهو القوي وهو المليك",
        "verse2": "وما له في ملكه شريك "
      },
      {
        "id": 18033,
        "verse1": "هو العزيز غيره الذليل",
        "verse2": "وهو المريد فعله الجميل"
      },
      {
        "id": 18034,
        "verse1": "وهو يعطي الملك من يشاء",
        "verse2": "ويرزق الخلق، له العطاء"
      },
      {
        "id": 18035,
        "verse1": "ألا له الخلق أجل والأمر",
        "verse2": "بيده الخير نعم والشر"
      },
      {
        "id": 18036,
        "verse1": "لا تفتح الشر إلينا أبدا",
        "verse2": "لا تغلقن بابا لخير سرمدا"
      },
      {
        "id": 18037,
        "verse1": "سدد جميع عملي ونيتي ",
        "verse2": "وطيبن عند اللقا منيتي "
      },
      {
        "id": 18038,
        "verse1": "مننت بالفضل العظيم تؤثر ",
        "verse2": "عبدك يا من حبنا تكثر"
      },
      {
        "id": 18039,
        "verse1": "يا ذا الذي ستر لي قبيحي",
        "verse2": "يا ذا الذي ظهرت بالمليح"
      },
      {
        "id": 18040,
        "verse1": "علي شكرك مدى الدوام",
        "verse2": "أنت الذي زينت لي أيامي"
      },
      {
        "id": 18041,
        "verse1": "أنت الذي نفست كل كرب",
        "verse2": "أنت الذي أتحفت كل قرب"
      },
      {
        "id": 18042,
        "verse1": "لك أيا منان كل شكري ",
        "verse2": "قلبي وروحي ثم شكر السر"
      },
      {
        "id": 18043,
        "verse1": "عندك قد خيمت ما بقيت",
        "verse2": "إذا بقيت بل إذا ونيت"
      },
      {
        "id": 18044,
        "verse1": "لك لساني شاكرا وقلبي",
        "verse2": "يا من حبا ما فات كل لب "
      },
      {
        "id": 18045,
        "verse1": "يا من تقدس عن الأضداد",
        "verse2": "صل على \"الها\" ثمرة العباد"
      },
      {
        "id": 18046,
        "verse1": "مسلما عليه والأصحاب",
        "verse2": "والآل والأزواج والأحباب"
      }
    ]
  },
  {
    "id": 1803,
    "diwanId": 18,
    "name": "ما أقدر الله أن يعطي بلا سبب",
    "rhyme": "ب",
    "content": [
      {
        "id": 18047,
        "verse1": "مَا أَقْدَرَ اللهَ أَنْ يُعْطِي بِلَا سَبَبِ ",
        "verse2": "مَا شَاءَهُ دُونَ كَيْدٍ لَا وَلَا نَصَبِ"
      },
      {
        "id": 18048,
        "verse1": "يُعْطِي وَيَمْنَعُ لَمْ يُسْأَلْ وَلَيْسَ لَهُ ",
        "verse2": "مُعَانِدٌ جَلَّ عَنْ شِرْكٍ وَعَنْ عَقِبِ"
      },
      {
        "id": 18049,
        "verse1": "يَا لَيْتَهُ صَبَّ وَبْلًا مِنْ عَطَاهُ عَلَى",
        "verse2": "ذَا الْعَبْدِ بَرْهَامَ حَتَّى يَنْقَضِي كُرَبِي"
      },
      {
        "id": 18050,
        "verse1": "يَا مَالِكَ النَّاسِ يَا مَوْلَايَ يَا صَمَدًا",
        "verse2": "هَبْ لِي انْكِشَافَ كُرُوبِي وَانْقِضَا خَبَبِي"
      },
      {
        "id": 18051,
        "verse1": "أَنْتَ الْإلَهُ وَأَنْتَ الْحَقُّ أَنْتَ غِيَا",
        "verse2": "ثِي أَعْطِنِي رَاحَتِي وَلْتَكْفِنِي تَعَبِي"
      },
      {
        "id": 18052,
        "verse1": "وَكُنْ نَصِيرِي وَكُنْ حِصْنِي الْقَوِي وَكَفَى ",
        "verse2": "مَوْلَى الْبَرِيَّةِ فِي الْعَادَاتِ وَالْكُرَبِ"
      },
      {
        "id": 18053,
        "verse1": "وَصَلِّ رَبِّ عَلَى طَهَ الْمُشَفَّعِ يَا ",
        "verse2": "مَوْلَايَ وَالصَّحْبِ ثُمَّ الآلِ ثُمَّ أَبِي"
      },
      {
        "id": 18054,
        "verse1": "وَسَلِّمَنْ رَبَّنَا وَلْتُعْطِ كَاتِبَهُ",
        "verse2": "بَرْهَامَ مَا رَامَ حِينًا دُونَمَا سَبَبِ"
      }
    ]
  },
  {
    "id": 1804,
    "diwanId": 18,
    "name": "أرجوزة التوبة",
    "rhyme": "ت",
    "content": [
      {
        "id": 18055,
        "verse1": "بِاسْمِ الْإلَهِ تُبْتُ إنِّي تُبْتُ",
        "verse2": "تُبْتُ لِمَا فَعَلْتُ أَوْ مَا قُلْتُِ"
      },
      {
        "id": 18056,
        "verse1": "تُبْتُ أَيَا رَحْمَنُ يَا رَحِيمُ",
        "verse2": "لَمْ تَرَنِي فِي حَالَةٍ تُلِيمُِ"
      },
      {
        "id": 18057,
        "verse1": "تُبْتُ أَيَا غَفَّارُ يَا حَنَّانُ ",
        "verse2": "تُبْتُ أَيَا رَبِّي وَيَا مَنَّانُ"
      },
      {
        "id": 18058,
        "verse1": "تُبْتُ حَقِيقَةً وَكَمْ نَكَثْتُ",
        "verse2": "لِي تَوْبَةً وَالْيَوْمَ حَقًّا تُبْتُ"
      },
      {
        "id": 18059,
        "verse1": "تُبْتُ لَدَى الْبَيْتِ وَبَيْنَ الْجَبَلَيْنْ",
        "verse2": "وَالْخَيْفِ وَالْمَشْعَرِ بَيْنَ الْعَلَمَيْنْ"
      },
      {
        "id": 18060,
        "verse1": "تُبْتُ لَدَى الْوُقُوفِ يَا حَلِيمُ",
        "verse2": "وَيَا عَفُوُّ رَبِّ يَا كَرِيمُِ"
      },
      {
        "id": 18061,
        "verse1": "أَنَا ضَعِيفٌ حَارَبَ الْجَبَّارَا",
        "verse2": "أَنَبْتُ يَوْمِي أَقْتَفِي الْأَبْرَارَا"
      },
      {
        "id": 18062,
        "verse1": "تُبْتُ وَكَمْ ضَيَّعْتُ حَقَّ اللهُِ",
        "verse2": "وَحَقَّ خَلْقِهِ بِلَا تَنَاه"
      },
      {
        "id": 18063,
        "verse1": "تَغْفِرُ لِي وَلْتَتَحَمَّلْ عَنِّي",
        "verse2": "اِغْفِرْ خَطِيَّتِي أَزِلْهَا مِنِّي"
      },
      {
        "id": 18064,
        "verse1": "تُبْتُ مِنَ الْأَقْوَالِ وَالْأَفْعَالُِِ",
        "verse2": "وَالْحَرَكَاتِ بَلْ مِنَ الْأَحْوَال"
      },
      {
        "id": 18065,
        "verse1": "جِئْتُ لَدَى الْبَيْتِ وَمَا اسْتَقَمْتُُِ",
        "verse2": "وَجِئْتُهُ وَجِئْتُهُ وَجِئْتُ"
      },
      {
        "id": 18066,
        "verse1": "وَجِئْتُهُ وَجِئْتُ سَادِسًا وَقَدُِْ",
        "verse2": "أَخْلَصْتُ تَوْبَتِي وَأَنْحُو لِلرَّشَدْ"
      },
      {
        "id": 18067,
        "verse1": "تُبْتُ لَدَى النَّبِيِّ بِالنَّبِيّ",
        "verse2": "تَرْفَعُنِي لِخُلُقٍ سَنِيِّ"
      },
      {
        "id": 18068,
        "verse1": "تُبْتُ وَلَا أَعُودُ لَا أَعُودُُِ",
        "verse2": "وَلَا يَكُونُ غَيْرُ مَا تُرِيدُ"
      },
      {
        "id": 18069,
        "verse1": "طَالَ اشْتِيَاقِي لِلْمَتَابِ تُبْتُُِ",
        "verse2": "فَإنَّنِي يَوْمِي لَهُ أَنَبْتُ"
      },
      {
        "id": 18070,
        "verse1": "قَدْ تَابَ سَمْعِي وَلِسَانِي وَالْبَصَرُِْ",
        "verse2": "وَتَابَ كُلُّ طَرَفٍ عَمَّا شَجَرْ"
      },
      {
        "id": 18071,
        "verse1": "رَجَعْتُ لِلْإلَهِ دَهْرِي لَمْ أَزَلُِْ",
        "verse2": "بِبَابِهِ مِنْ أَبَدٍ إلَى الْأَزَل"
      },
      {
        "id": 18072,
        "verse1": "ثُمَّ الصَّلَاةُ وَالسَّلَامُ سَرْمَدَاُِ",
        "verse2": "عَلَى النَّبِيِّ الْهَاشِمِيِّ مَنْ هَدَى"
      },
      {
        "id": 18073,
        "verse1": "وَآلِهِ وَصَحْبِهِ الْأَخْيَارِ",
        "verse2": "مَا كُوِّرَ اللَّيْلُ عَلَى النَّهَارِ"
      }
    ]
  },
  {
    "id": 1805,
    "diwanId": 18,
    "name": "سَأَلْتُكَ يَا إلَهِي فِي حَيَاتِي",
    "rhyme": "ت",
    "content": [
      {
        "id": 18074,
        "verse1": "سَأَلْتُكَ يَا إلَهِي فِي حَيَاتِي",
        "verse2": "لِتَكْفِيَنِي مَكَائِدَ لِلْعُدَاةُِِ"
      },
      {
        "id": 18075,
        "verse1": "كَثِّرْ رِزْقَ عَبْدِكَ وَاقْضِ دَيْنِي",
        "verse2": "وَلَا أَبَدًا أَرَى جَوْرَ الْوُلَاةُِِ"
      },
      {
        "id": 18076,
        "verse1": "وَلَا تُشْمِتْ عَدُوًّا أَوْ حَسُودًا",
        "verse2": "سِوَاهُ فِي الْحَوَاضِرِ وَالْبُدَاةُِ"
      },
      {
        "id": 18077,
        "verse1": "وَأَكْثِرْ غَيْثَ سَالُمٍ مَعْ ذَوِيهَا",
        "verse2": "وَبَارِكْ فِي الْمَزَارِعِ وَالنَّبَاتِ"
      },
      {
        "id": 18078,
        "verse1": "وَسَخِّرْ لِي جَمِيعَ الْخَلْقِ هَبْ لِيْ",
        "verse2": "مُرَادِي فِي أَقَالِيمِ الْجِهَاتِْ"
      },
      {
        "id": 18079,
        "verse1": "وَأَصْلِحْ كُلَّ أَزْوَاجِي  وَنَسْلِيُ",
        "verse2": "وَأَيْتَامًا بِحِجْرِي لِلْوَصَاةُِِ"
      },
      {
        "id": 18080,
        "verse1": "وَجَازِ الصَّحْبَ وَالْأَحْبَابَ خَيْرًا",
        "verse2": "سِوَى مَنْ صَدَّ كَالْأَعْدَا الشُّنَاةِ"
      },
      {
        "id": 18081,
        "verse1": "وَتَصْحَبُنِي بِأَسْفَارِي وَكُنْ لِي ",
        "verse2": "مُضِيفًا لَا أَرَى بَغْيَ الْبُغَاةِ"
      },
      {
        "id": 18082,
        "verse1": "هِبَاتُكَ لِي أَدِمْهَا لِي وَزِدْهَا",
        "verse2": "وَتَمِّمْ لِي الْجَمَالَ إلَى الْمَمَاتِ"
      },
      {
        "id": 18083,
        "verse1": "وَتُسْعِدُنِي لِكَيْ أَحْيَا سَعِيدًا",
        "verse2": "كَذَا بَعْدَ الْمَمَاتِ مَعَ التُّقَاةِ"
      },
      {
        "id": 18084,
        "verse1": "صَلَاةُ اللهِ يَصْحَبُهَا سَلَامٌ عَلَى ",
        "verse2": "الْمُخْتَارِ مِنْ خَيْرِ اللِّدَاتِ"
      }
    ]
  },
  {
    "id": 1806,
    "diwanId": 18,
    "name": "توسل على حروف (ولن يجعل الله للكفرين على المؤمنين سبيلا)",
    "rhyme": "ت",
    "content": [
      {
        "id": 18085,
        "verse1": "والله لن يجعل المولى أبدا",
        "verse2": "أدنى سبيل لنا ولم أقل فنداُِِ"
      },
      {
        "id": 18086,
        "verse1": "لا تحسبونا بقينا لا استناد لنا",
        "verse2": "فالله أفضل موجود يرى سنداُِِ"
      },
      {
        "id": 18087,
        "verse1": "نعلو عليهم ولا يعلون ليس لهم",
        "verse2": "ولى ونعبد ربا واحدا صمداُِ"
      },
      {
        "id": 18088,
        "verse1": "يكفي العدو ويكفي كيده وكفى",
        "verse2": "فهو القوي العزيز المعتلي أبدا"
      },
      {
        "id": 18089,
        "verse1": "جعلته جنة بني وبينهم",
        "verse2": "لاضرنا أبدا كيد الذي مرداِْ"
      },
      {
        "id": 18090,
        "verse1": "على الجليل اعتمادي والوسيلة لي",
        "verse2": "طه الأمين الذي قد اهتدى وهدى"
      },
      {
        "id": 18091,
        "verse1": "لمدحه ولحبيبه صفوت له ",
        "verse2": "وهوسر  الذي يصطفيه الحق إذ عبدا"
      },
      {
        "id": 18092,
        "verse1": "الله يرفع رايات الأمين وقد ",
        "verse2": "قرت لي العين حين الكل قد رشدا"
      },
      {
        "id": 18093,
        "verse1": "للمصطفى حركاتي والسكون له ",
        "verse2": "بل كل كلي للهادي الذي حمدا"
      },
      {
        "id": 18094,
        "verse1": "له اشتياقي فلم أشتق لغانية",
        "verse2": "هل من سبيل لوصل بعدما فقداِ"
      },
      {
        "id": 18095,
        "verse1": "هو الحبيب الذي أشواقه ملأت",
        "verse2": "قلبي ومن حبه حب السوى فسدا"
      },
      {
        "id": 18096,
        "verse1": "لي حققن رجاء طالما علقت",
        "verse2": "به المطامع وامح الحقد والحسدا"
      },
      {
        "id": 18097,
        "verse1": "لا ضرنا شر أعداء كفيتهم",
        "verse2": "ولتكف شر حسود نافث عقدا"
      },
      {
        "id": 18098,
        "verse1": "كسر جموع عدى الإسلام قاطبة",
        "verse2": "بالمصطفى المنتقى يا خير من قصدا"
      },
      {
        "id": 18099,
        "verse1": "أيد بنا كلمة التوحيد ـهل بنا",
        "verse2": "نارة الحق يبقى الكل متحدا"
      },
      {
        "id": 18100,
        "verse1": "فلا يرى منهج في الحق غير صرا",
        "verse2": "ط المصطفى راح منصورا علا وبدا"
      },
      {
        "id": 18101,
        "verse1": "رب العباد تعز الدين إن لنا ",
        "verse2": "رجا كبيرا لفضل منك قد عهدا"
      },
      {
        "id": 18102,
        "verse1": "يا رب هب لي زمانا لا أرى علما",
