      {
        "id": 18103,
        "verse1": "نرى زوالا لعز الكفر قد هدمت",
        "verse2": "تلك الرسوم ويعلو الدين منفردا"
      },
      {
        "id": 18104,
        "verse1": "عليك حقا كما أخبرتنا كرما",
        "verse2": "نصر لنا فأنلنا الفوز والرشدا"
      },
      {
        "id": 18105,
        "verse1": "لتعط عبدك ما يغنيه ما طلعت",
