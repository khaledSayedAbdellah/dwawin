      },
      {
        "id": 18113,
        "verse1": "سألت ربي بالأسما وأعظمها",
        "verse2": "وحضرة المصطفى وسر ما وجدا"
      },
      {
        "id": 18114,
        "verse1": "بالذات بالنور والفرقان تسمعنا",
        "verse2": "فقد دعوناك حين الكل قد رقدا"
      },
      {
        "id": 18115,
        "verse1": "يا رب صل على المختار سيدنا",
        "verse2": "وسلمن عليه ثم من ولدا"
      },
      {
        "id": 18116,
        "verse1": "لتبلغ المصطفى مني السلام على",
        "verse2": "ماحي الضلال واعط الفيض والمددا"
      },
      {
        "id": 18117,
        "verse1": "أتمم صلاتي وتسليمي عليه بذكـ",
        "verse2": " ـر الآل و الصحب ثم من قفا وهدى"
      }
    ]
  },
  {
    "id": 1807,
    "diwanId": 18,
    "name": "فأحمد ربي ثم أمدح سيدا",
    "rhyme": "ت",
    "content": [
      {
        "id": 18118,
        "verse1": "فَأَحْمَدُ رَبِّي ثُمَّ أَمْدَحُ سَيِّدًا",
        "verse2": "تَوَسَّطَ لِلْأَكْوَانِ بَحْرًا قَدَ ازْبَدَاُِِ"
      },
      {
        "id": 18119,
        "verse1": "فَقَدْ طَالَمَا أَسْدَى لِأَفْقَرِ أَعْبُدٍ",
        "verse2": "مَوَاهِبَ لَنْ تُحْصَى عَطَاءً مُؤَكَّدَاُِِ"
      },
      {
        "id": 18120,
