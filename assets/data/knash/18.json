      },
      {
        "id": 18123,
        "verse1": "أَلَمْ يُعْجِزِ الْأَلْبَابَ آيٌ تَوَاتَرَتْ",
        "verse2": "لَدَى الْمَوْلِدِ السَّامِي وَحَسْبُكَ مَوْلِدَا"
      },
      {
        "id": 18124,
        "verse1": "وَبَعْدُ تَبَدَّتْ فِي الرَّضَاعِ غَرَائِبٌ ",
        "verse2": "وَحُسْنُ مُحَيَّا الْبَدْرِ يَكْفِيهِ مَحْمَدَا"
      },
      {
        "id": 18125,
        "verse1": "وَفِيٌّ وَوَهَّابٌ وَلِيٌّ وَنَاصِرٌ ",
        "verse2": "تَقِيٌّ نَقِيٌّ سَيْفُ مَوْلَاهُ فِي الْعِدَى"
      },
      {
        "id": 18126,
        "verse1": "عَفِيفٌ وَسَمْحٌ أَرْيَحِيٌّ أَخُو سَخًا",
        "verse2": "شُجَاعٌ كَرِيمٌ رَاحِمٌ هَبْهُ مُنْجِدَا"
      },
      {
        "id": 18127,
        "verse1": "فَأَوْصَافُهُ غَيْبِيَّةٌ عِنْدَ رَبِّنَا ",
        "verse2": "لَقَدْ جَهِلَ الْمُخْتَارُ غَيْرَ الذِ ابْتَدَا"
      },
      {
        "id": 18128,
        "verse1": "أُحِبُّ رَسُولَ اللهِ أَرْجُو لِقَاءَهُ",
        "verse2": "وَمِنْ قَبْلُ بِالْأَمْدَاحِ مَا زِلْتُ مُنْشِدَا"
      },
      {
        "id": 18129,
        "verse1": "تَيَقَّنْتُ أَنْ بِالْحُبِّ أَلْقَى نَبِيَّنَا",
        "verse2": "وَأَظْفَرَ بِالْحَاجَاتِ عَبْدًا مُسَوَّدَا"
      },
      {
        "id": 18130,
        "verse1": "َكَانَ نَبِيًّا حِينَ آدَم ُطِينَةٌ",
        "verse2": "وَمَاءٌ فَفَاقَ الْمُرْسَلِينَ مُمَجَّدَا"
      },
      {
        "id": 18131,
        "verse1": "نُجُومٌ وَذَاكَ الشَّمْسُ مِنْهُ تَكَسَّبُوا",
        "verse2": "عُلُومًا وَأَسْرَارًا وَآيًا وَسُؤْدَدَا"
      },
      {
        "id": 18132,
        "verse1": "يَمِينُ رَسُولِ اللهِ يُمْنُ أَحِبَّةٍ",
